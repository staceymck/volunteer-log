<%= form_for volunteer do |f| %>
  <%= f.label :first_name %><br>
  <%= f.text_field :first_name, required: true %><br>
  <%= f.label :last_name %><br>
  <%= f.text_field :last_name, required: true %><br>
  <%= f.label :phone %><br>
  <%= f.text_field :phone %><br>
  <%= f.label :email %><br>
  <%= f.email_field :email %><br>
  <%= f.label :occupation %><br>
  <%= f.text_field :occupation %><br>
  <%= f.label :employer %><br>
  <%= f.text_field :employer %><br>
  <%= f.label :birthday %><br>
  <%= f.date_field :birthday %><br>
  <%= f.label :age_group %><br>
  <%= f.select :age_group, [["Under 18", "under_eighteen"], ["18-21", "over_eighteen"], ["Over 21", "over_twenty_one"]] %><br>
  <%= f.label :background_check_status %><br>
  <%= f.select :background_check_status, [["Incomplete", "incomplete"], ["Approved", "approved"], ["Flagged", "flagged"]] %><br>
  <%= f.label :photo, "Photo link" %><br>
  <%= f.url_field :photo %><br>
  <%= f.label :interests %><br>
  <%= f.text_area :interests %><br>
  <%= f.submit "Save" %>
<% end %>